// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model Instance {
  id      String    @id @db.Uuid
  name    String    @unique
}

model AccessKey {
  id      String    @id @db.Uuid
  name    String
  key     Bytes
  scopes  String[]
}

model Settings {
  name    String    @id
  text    String    @default("")
  blob    Bytes     @default("")
}
